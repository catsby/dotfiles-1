{
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.disableKind": true,
	"coc.preferences.formatOnSaveFiletypes": ["go"],
  "coc.preferences.snippetIndicator": "►",
  "suggest.snippetIndicator": "►",
  "coc.preferences.diagnostic.hoverTarget": "float",
	"coc.preferences.jumpCommand": "edit",
  "suggest.noselect": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "?",
  "languageserver": {
    "golang": {
      "command": "/Users/clint/Go/bin/gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"],
      "args": ["-remote=auto"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "go.goplsOptions": {
    "staticcheck": true
  }
}
